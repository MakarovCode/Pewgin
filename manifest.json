{
  "name": "Pewgin",
  "version": "1.0",
  "description": "Let's do this everybody!",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "jquery.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "manifest_version": 2
}
